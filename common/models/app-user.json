{
  "name": "AppUser",
  "plural": "appusers",
  "base": "User",
  "idInjection": false,
  "options": {
    "validateUpsert": true,
    "emailVerificationRequired": false
  },
  "mongodb": {
    "collection": "AppUser"
  },
  "mixins": {
    "Timestamp": true,
    "OwnId": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string"
    },
    "lastAccess": {
      "type": "date"
    },
    "email": {
      "type": "string",
      "required": true,
      "index": true
    },
    "description": {
      "type": "string"
    },
    "genderShort": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "image": {
      "type": "embedsOne",
      "model": "Image",
      "property": "imagePreview"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "userId"
    },
    "gender": {
      "type": "belongsTo",
      "model": "Gender",
      "foreignKey": "genderShort"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
