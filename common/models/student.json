{
  "name": "Student",
  "plural": "students",
  "base": "Account",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "lastAccess": {
      "type": "date"
    },
    "description": {
      "type": "string"
    },
    "firstPassword": {
      "type": "boolean",
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "gender": {
      "type": "belongsTo",
      "model": "Gender",
      "foreignKey": "gender_id"
    },
    "courses": {
      "type": "hasMany",
      "model": "CourseStudent",
      "foreignKey": "student_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "property": "patchOrCreate",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
