{
  "name": "CourseModule",
  "plural": "course-modules",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "position": {
      "type": "number",
      "required": true
    },
    "description": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "instructor": {
      "type": "belongsTo",
      "model": "Instructor",
      "foreignKey": "instructor_id"
    },
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": "course_id"
    },
    "videos": {
      "type": "hasMany",
      "model": "ModuleVideo",
      "foreignKey": "module_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
