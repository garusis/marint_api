{
  "name": "Course",
  "plural": "courses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "isPublished": {
      "type": "boolean",
      "index": true,
      "default": true
    },
    "price": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "image": {
      "type": "embedsOne",
      "model": "Image",
      "property": "imagePreview"
    },
    "modules": {
      "type": "embedsMany",
      "model": "CourseModule",
      "property": "moduleList"
    }
  },
  "acls": [],
  "methods": {}
}
