{
  "name": "Course",
  "plural": "courses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "isPublished": {
      "type": "boolean",
      "default": false
    },
    "price": {
      "type": "number",
      "postgresql": {
        "dataType": "REAL"
      },
      "required": true
    },
    "instructor_id": {
      "type": "number",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "image": {
      "type": "hasOne",
      "model": "Image",
      "polymorphic": {
        "foreignKey": "imageable_id",
        "discriminator": "imageable_type"
      }
    },
    "instructor": {
      "type": "belongsTo",
      "model": "Instructor",
      "foreignKey": "instructor_id"
    },
    "modules": {
      "type": "hasMany",
      "model": "CourseModule",
      "foreignKey": "course_id"
    },
    "students": {
      "type": "hasMany",
      "model": "Student",
      "foreignKey": "course_id",
      "through": "CourseStudent"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
