{
  "name": "ModuleVideo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "url": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "module": {
      "type": "belongsTo",
      "model": "CourseModule",
      "foreignKey": "module_id"
    },
    "uploader": {
      "type": "belongsTo",
      "model": "Instructor",
      "foreignKey": "instructor_id",
      "options": {
        "defineUploader": true
      }
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "polymorphic": {
        "foreignKey": "publication_id",
        "discriminator": "publication_type"
      }
    }
  },
  "acls": [],
  "methods": {}
}
