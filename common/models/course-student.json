{
  "name": "CourseStudent",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "course_id": {
      "type": "number",
      "required": true,
      "index": true
    },
    "student_id": {
      "type": "number",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": "course_id"
    },
    "modules": {
      "type": "hasMany",
      "model": "CourseModule",
      "foreignKey": "module_id",
      "through": "ModuleStudent"
    },
    "student": {
      "type": "belongsTo",
      "model": "Student",
      "foreignKey": "student_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "property": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
