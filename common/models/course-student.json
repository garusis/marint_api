{
  "name": "CourseStudent",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {},
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": "course_id"
    },
    "modules": {
      "type": "hasMany",
      "model": "CourseModule",
      "foreignKey": "module_id",
      "through": "ModuleStudent"
    },
    "student": {
      "type": "belongsTo",
      "model": "Student",
      "foreignKey": "student_id"
    }
  },
  "acls": [],
  "methods": {}
}
